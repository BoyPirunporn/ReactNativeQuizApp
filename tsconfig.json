{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ],
      "@/*": [
        "./*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    }
  },
  "files": [
    "globals.d.ts"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expo.env.d.ts",
    "App.tsx.test",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
