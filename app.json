{
  "expo": {
    "name": "questionandanswer",
    "slug": "question-interview",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#76FD98"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prpasn.question-interview"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#76FD98"
      },
      "package": "com.myquestion",
      "googleServicesFile":"./google-services.json"
    },
    "plugins":[
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "d4a76824-4b05-4cad-bbe2-a23668715a68"
      }
    }
  }
}
